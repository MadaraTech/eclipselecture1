import java.util.Arrays;

public class Starter {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int[] arrays = new int[3];

		System.out.println("Array length is: " +arrays.length);
		
		arrays[0] = 1;
		arrays[4] = 10;
		
		System.out.println(Arrays.toString(arrays));
	}

}
